automatic_id 1
script = dev.lua
inputs = [ tick ]
# f - for flags
# s - for state
# t - direction
# v - anim frame (0 static, 1-2 walking, 3 bonk)
# x,y,t - interp-specific vars
vars = { u8 f = 0 u8 v = 0 u8 t = 0 u8 s = 0; i16 x = 0 i16 y = 0 u32 T = 0 }
texture = dev.png
frame_controller = v
type_controller = t
#
offset_x_controller = x
offset_y_controller = y
interp_timestamp_controller = T
# 100 ms sinc dev moves every tick at 10tps
interp_takes = 100